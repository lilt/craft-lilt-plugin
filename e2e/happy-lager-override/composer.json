{
  "require": {
    "php": ">=7.0.0",
    "craftcms/cms": "3.7.61",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "^2.8.7",
    "lilt/craft-lilt-plugin": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    }
  }
}
